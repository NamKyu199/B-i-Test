{
  "name": "SigIn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "cache:clean": "yarn cache clean",
    "env:production": "ENVFILE=.env.production",
    "env:staging": "ENVFILE=.env.staging",
    "env:development": "ENVFILE=.env.development",
    "ios:pod-install": "cd ios && pod install && cd ..",
    "ios:pod-reset": "cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf Podfile.lock && rm -rf ~/Library/Developer/Xcode/DerivedData && pod deintegrate && pod setup && pod install && cd ..",
    "ios:debug-production": "yarn env:production react-native run-ios --mode=Debug --scheme \"crmProduction\"",
    "ios:debug-staging": "yarn env:staging react-native run-ios --mode=Debug --scheme \"crmStaging\"",
    "ios:debug-development": "yarn env:development react-native run-ios --mode=Debug --scheme \"crmDevelopment\"",
    "ios:release-production": "yarn cache:clean && yarn env:production react-native run-ios --mode=Release --scheme \"crmProduction\"",
    "ios:release-staging": "yarn cache:clean && yarn env:staging react-native run-ios --mode=Release --scheme \"crmStaging\"",
    "ios:release-development": "yarn cache:clean && yarn env:development react-native run-ios --mode=Release --scheme \"crmDevelopment\"",
    "android:debug-production": "yarn env:production react-native run-android --mode=productiondebug",
    "android:debug-staging": "yarn env:staging react-native run-android --mode=stagingdebug",
    "android:debug-development": "yarn env:development react-native run-android --mode=developmentdebug",
    "android:release-production": "yarn env:production react-native run-android --mode=productionrelease",
    "android:release-staging": "yarn env:staging react-native run-android --mode=stagingrelease",
    "android:release-development": "yarn env:development react-native run-android --mode=developmentrelease",
    "codepush:android-production": "appcenter codepush release-react -a midu-ledhcg/CRM-ANDROID -d Production",
    "codepush:android-staging": "appcenter codepush release-react -a midu-ledhcg/CRM-ANDROID -d Staging",
    "codepush:ios-production": "appcenter codepush release-react -a midu-ledhcg/CRM-IOS -d Production",
    "codepush:ios-staging": "appcenter codepush release-react -a midu-ledhcg/CRM-IOS -d Staging"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "^2.3.2",
    "@ant-design/react-native": "^5.0.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-arrow-functions": "^7.23.3",
    "@babel/plugin-transform-shorthand-properties": "^7.23.3",
    "@babel/plugin-transform-template-literals": "^7.23.3",
    "@faker-js/faker": "^8.4.1",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "^1.22.0",
    "@react-native-camera-roll/camera-roll": "^7.4.1",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-community/segmented-control": "^2.2.2",
    "@react-native-community/slider": "^4.5.0",
    "@react-native-picker/picker": "^2.6.1",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@reduxjs/toolkit": "^2.2.1",
    "@shopify/react-native-skia": "^0.1.241",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "axios": "^1.6.7",
    "formik": "^2.4.5",
    "i18next": "^23.8.2",
    "iconsax-react-native": "^0.0.8",
    "lottie-react-native": "^6.6.0",
    "react": "18.2.0",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^14.0.5",
    "react-native": "0.73.4",
    "react-native-code-push": "^8.2.1",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.12.0",
    "react-native-gesture-handler": "2.14.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-month-year-picker": "^1.9.0",
    "react-native-paper": "^5.12.3",
    "react-native-qrcode-styled": "^0.3.0",
    "react-native-reanimated": "^3.7.1",
    "react-native-redash": "^18.1.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-step-indicator": "^1.0.3",
    "react-native-svg": "^14.1.0",
    "react-native-ui-lib": "7.10.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
